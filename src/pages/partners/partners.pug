extends ../../layout/layout

block variables
    - var title = 'Партнёры - Сильные идеи для нового времени'
    - var keywords = 'Сильные идеи для нового времени, партнёры'
    - var description = 'Приглашаем вас принять участие в работе форума «Сильные идеи для нового времени», который состоится в октябре 2020 года в Сочи на территории Парка науки и искусства «Сириус».'


block main

        div#default-page-top
            .container
                nav.breadcrumbs
                    a(href="/") Главная
                    p /
                    p Форум
                h1 Партнеры

        div#partners-page.page-content
            .container
                nav.page-navigation
                    a(href="#terms-of-participation") Заявка на участие
                    br
                    a(href="#consideration-of-the-application") Рассмотрение заявки
                    br
                    a(href="#packages-and-participation-fees") Пакеты и стоимость участия
                    br
                    a(href="#quotas") Квотирование
                    br
                    a(href="#certificate-of-absence-of-COVID-19") Справка об отсутствии COVID-19
                    br
                    a(href="#contacts") Контакты

            section.title-partners-section
                .container
                    h2 Титульные партнеры

                    .partners-list
                        a(href="https://www.ranepa.ru/")
                            img(src="/images/partner-1.svg")
                        a(href="https://www.hse.ru/en/")
                            img(src="/images/partner-2.jpg")

            section.general-partners-section
                .container
                    h2 Генеральные партнеры

                    .partners-list
                        a(href="https://2035.university/")
                            img(src="/images/partner-3.png")

            section.official-partners-section
                .container
                    h2 Официальные партнеры

                    .partners-list
                        a(href="https://tass.ru/")
                            img(src="/images/partner-4.svg")

            section.organisation-partners-section
                .container
                    h2 Партнеры по организации зоны делового общения
                    .partners-list
                        a(href="https://www.worldbank.org/")
                            img(src="/images/partner-5.png")

            section.program-partners-section
                .container
                    h2 Партнеры по деловой программе
                    .partners-list
                        a(href="https://veb.ventures/ru")
                            img(src="/images/partner-6.png")

            section.cultural-and-sports-partners-section
                .container
                    h2 Партнеры культурной и спортивной программы
                    .partners-list
                        a(href="https://rsv.ru/")
                            img(src="/images/partner-7.jpg")
                        a(href="http://www.rspp.ru/")
                            img(src="/images/partner-8.png")


